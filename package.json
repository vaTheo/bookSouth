{
  "name": "book-south",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "yarn workspaces run start",
    "dev": "concurrently \"yarn workspace @book-south/frontend dev\" \"yarn workspace @book-south/backend dev\"",
    "dev:frontend": "yarn workspace @book-south/frontend dev",
    "dev:backend": "yarn workspace @book-south/backend dev",
    "build": "yarn workspaces run build",
    "lint": "yarn workspaces run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "yarn workspaces run clean"
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/vaTheo/bookSouth.git",
  "author": "vaTheo <vincenta.theo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.4.2"
  },
  "dependencies": {
    "concurrently": "^9.1.0",
    "typescript": "^5.7.2"
  }
}
